  <h4><%= current_user.email %></h4>

  <h3>Users list</h3>
  <%= turbo_stream_from 'users' %>
  <div id='users'>
    <%= render @users %>
  </div>

  <h3>Rooms list</h3>
  <%= turbo_stream_from 'rooms' %>
  <div id='rooms'>
    <%= render @rooms %>
  </div>

  <%= render 'layouts/new_room_form' %>

  <% if @single_room %>
    <h4><%= @single_room.name %></h4>
  <% end %>
